{
  "name": "tracker",
  "version": "0.1.0",
  "description": "Tracker application build with Electron",
  "author": {
    "name": "Dmytro Shcherbonos",
    "email": "dimaus.sch@gmail.com"
  },
  "main": "src/main",
  "repository": {},
  "license": "ISC",
  "scripts": {
    "update-core": "git submodule update --remote --merge",
    "fetch-core": "git submodule update --init --recursive",
    "preinstall": "cd ./tracker && npm install",
    "electron-react": "concurrently \"cd ./tracker && BROWSER=none npm start\" \"wait-on http://localhost:3000 && bozon start\""
  },
  "devDependencies": {
    "bozon": "1.3.3",
    "concurrently": "^7.4.0",
    "jest": "29.1.1",
    "spectron": "19.0.0",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "com.electron.tracker",
    "win": {
      "publish": null
    },
    "mac": {
      "publish": null,
      "category": "your.app.category.type"
    }
  }
}
